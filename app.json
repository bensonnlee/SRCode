{
  "expo": {
    "name": "SRCode",
    "slug": "SRCode",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/logo_1024.png",
    "scheme": "srcode",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.SRCode",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/logo/logo_1024.png",
        "monochromeImage": "./assets/logo/logo_1024.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.SRCode",
      "permissions": [
        "android.permission.WRITE_SETTINGS"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/logo/logo_40.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo/logo_256.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store",
      "expo-brightness"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4304638d-50cf-42ba-956d-d0f97126636a"
      }
    }
  }
}
